    "basePath": "/",
    "paths": {
        "/v1/example/testget/{name}": {
            "get": {
                "description": "用GET method 測試api",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "EX1"
                ],
                "summary": "用GET method 測試api",
                "parameters": [
                    {
                        "type": "string",
                        "description": "token",
                        "name": "Authorization",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "default": "Jim",
                        "description": "名字1",
                        "name": "name",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "default": 18,
                        "description": "年紀",
                        "name": "old",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "ok",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/apimodel.Response"
                        }
                    }
                }
            }
        },
        "/v1/example/testpost": {
            "post": {
                "description": "用POST method 測試api",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "EX2"
                ],
                "summary": "用POST method 測試api",
                "parameters": [
                    {
                        "description": "body",
                        "name": "sendBody",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/apimodel.ExamplePost"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "ok",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/apimodel.Response"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "apimodel.ExamplePost": {
            "type": "object",
            "required": [
                "name"
            ],
            "properties": {
                "name": {
                    "type": "string",
                    "example": "jim"
                },
                "old": {
                    "type": "integer",
                    "example": 30
                }
            }
        },
        "apimodel.Response": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "data": {
                    "type": "object"
                },
                "message": {
                    "type": "string"
                }
            }
        }
    }
